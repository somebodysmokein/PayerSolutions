<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>Consumer</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>  
  <servlet>
    <description></description>
    <display-name>ViewProvider</display-name>
    <servlet-name>ViewProvider</servlet-name>
    <servlet-class>com.TCS.ViewProvider</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ViewProvider</servlet-name>
    <url-pattern>/getProv</url-pattern>
  </servlet-mapping>
   <servlet>
    <description></description>
    <display-name>PullProviderUpdates</display-name>
    <servlet-name>PullProviderUpdates</servlet-name>
    <servlet-class>com.TCS.PullProviderUpdates</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>PullProviderUpdates</servlet-name>
    <url-pattern>/loadProv</url-pattern>
  </servlet-mapping> 
    <servlet>
    <description></description>
    <display-name>getDates</display-name>
    <servlet-name>getDates</servlet-name>
    <servlet-class>com.TCS.getCycleDates</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>getDates</servlet-name>
    <url-pattern>/getDate</url-pattern>
  </servlet-mapping> 
</web-app>
